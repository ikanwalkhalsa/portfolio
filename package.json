{
  "name": "kanwalpreet-portfolio",
  "version": "1.0.0",
  "description": "Kanwalpreet Singh - Machine Learning Engineer Portfolio",
  "private": true,
  "scripts": {
    "dev": "next dev -p 5000",
    "dev:watch": "concurrently \"next dev -p 5000\" \"nodemon\"",
    "build": "next build",
    "start": "next start -p 5000",
    "start:prod": "next start -p 3000",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "refresh": "node scripts/refresh-browser.js",
    "docker:build": "docker build -t portfolio .",
    "docker:run": "docker run -p 3000:3000 portfolio",
    "docker:compose": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "clean": "rm -rf .next node_modules/.cache",
    "postinstall": "npm run type-check",
    "deploy": "./scripts/deploy.sh",
    "deploy:docker": "./scripts/deploy.sh docker",
    "deploy:compose": "./scripts/deploy.sh compose",
    "health": "./scripts/deploy.sh health",
    "analyze": "ANALYZE=true npm run build",
    "validate": "npm run type-check && npm run lint"
  },
  "dependencies": {
    "@tensorflow/tfjs": "^4.22.0",
    "@tensorflow/tfjs-node": "^4.22.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "framer-motion": "^10.12.16",
    "js-yaml": "^4.1.0",
    "lucide-react": "^0.294.0",
    "next": "^14.0.0",
    "postcss": "^8.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recharts": "^3.2.0",
    "tailwind-merge": "^2.0.0",
    "tailwindcss": "^3.3.0",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.0.0"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "eslint": "^8.0.0",
    "eslint-config-next": "^14.0.0",
    "nodemon": "^3.1.10"
  }
}
